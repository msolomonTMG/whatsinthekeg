<header id="top" class="header">
    <div class="container" style="margin-top:30px;">
    <div class="text-vertical-center">
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-4">
                            <span class="pull-left" data-toggle="tooltip" data-placement="bottom" title="On tap since">
                                <i class="fa fa-calendar-o"></i> <span id="techBeerTime"></span>
                            </span>
                        </div>
                        <div class="col-xs-4">
                            Tech
                        </div>
                        <div class="col-xs-4">
                            <span class="pull-right" id="techBeerLikesPosition">

                            </span>
                        </div>  
                    </div>                
                </div>
                <div class="panel-body">
                    <h4 id="techBeerName">
				        <% for (beer in beers) { %>
				        	<% if (beers[beer].currentTechBeer == true) { %> <%= beers[beer].name %> <% } %>
				        <% } %>
                    </h4> 
                    <div class="portfolio-item">
                        <a href="#" id="techBeerSite" target="_blank">
                            <img id="techBeerImg" class="img-portfolio img-responsive" src="https://media.giphy.com/media/X530jPOZXRMwo/giphy.gif">
                        </a>
                    </div>
                </div>
                <div class="panel-footer" id="techBeerCommentsPosition" style="overflow:hidden;">

                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-4">
                            <span class="pull-left" data-toggle="tooltip" data-placement="bottom" title="On tap since">
                                <i class="fa fa-calendar-o"></i> 
                                <span id="editBeerTime">
                                    <% for (beer in beers) { %>
                                        <% if (beers[beer].currentEditBeer == true) { %> <%= beers[beer].lastOnTap %> <% } %>
                                    <% } %>
                                </span>
                            </span>
                        </div>
                        <div class="col-xs-4">
                            Edit
                        </div>
                        <div class="col-xs-4">
                            <span class="pull-right" id="editBeerLikesPosition">
                                
                            </span>
                        </div>  
                    </div>   
                </div>
                <div class="panel-body">
                    <h4 id="editBeerName">
				        <% for (beer in beers) { %>
				        	<% if (beers[beer].currentEditBeer == true) { %> <%= beers[beer].name %> <% } %>
				        <% } %>
                    </h4> 
                    <div class="portfolio-item">
                        <a href="#" id="editBeerSite" target="_blank">
                            <img id="editBeerImg" class="img-portfolio img-responsive" src="https://media.giphy.com/media/X530jPOZXRMwo/giphy.gif">
                        </a>
                    </div>
                </div>
                <div class="panel-footer" id="editBeerCommentsPosition" style="overflow:hidden;">
                    
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-4">
                            <span class="pull-left" data-toggle="tooltip" data-placement="bottom" title="On tap since">
                                <i class="fa fa-calendar-o"></i> 
                                <span id="jtBeerTime">
                                    <% for (beer in beers) { %>
                                        <% if (beers[beer].currentJtBeer == true) { %> <%= beers[beer].lastOnTap %> <% } %>
                                    <% } %>
                                </span>
                            </span>
                        </div>
                        <div class="col-xs-4">
                            JackThreads
                        </div>
                        <div class="col-xs-4">
                            <span class="pull-right" id="jtBeerLikesPosition">
                                
                            </span>
                        </div>  
                    </div>   
                </div>
                <div class="panel-body">
                    <h4 id="jtBeerName">
				        <% for (beer in beers) { %>
				        	<% if (beers[beer].currentJtBeer == true) { %> <%= beers[beer].name %> <% } %>
				        <% } %>
                    </h4> 
                    <div class="portfolio-item">
                        <a href="
                            <% for (beer in beers) { %>
                                <% if (beers[beer].currentJtBeer == true) { %> <%= beers[beer].website %> <% } %>
                            <% } %>
                        " id="jtBeerSite" target="_blank">
                            <img id="jtBeerImg" class="img-portfolio img-responsive" src="
                                    <% for (beer in beers) { %>
                                        <% if (beers[beer].currentJtBeer == true) { %> <%= beers[beer].imgurl %> <% } %>
                                    <% } %>
                            ">
                        </a>
                    </div>
                </div>
                <div class="panel-footer" id="jtBeerCommentsPosition" style="overflow:hidden;">
                    
                </div>
            </div>
        </div>
    </div>
</div>
</header>
